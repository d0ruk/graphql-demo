{
  "name": "graphql-demo",
  "private": true,
  "version": "1.0.0",
  "main": "build/index.js",
  "description": "graphql/express server with dockerized postgres",
  "scripts": {
    "start": "npm run txpile && NODE_ENV=production node .",
    "txpile": "babel -d build --minified src",
    "dev": "node-dev src",
    "seed": "node -r @babel/register seed.js",
    "inspect": "node -r @babel/register --inspect-brk src",
    "test": "eslint src seed.js"
  },
  "keywords": [
    "graphql",
    "postgres",
    "sequelize",
    "docker"
  ],
  "author": "Doruk Kutlu",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.0.5",
    "apollo-server-express": "^2.0.4",
    "bcrypt": "^3.0.1",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "pg": "^7.4.3",
    "sequelize": "^4.39.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "faker": "^4.1.0",
    "node-dev": "^3.1.3"
  },
  "engines": {
    "node": ">=8.9.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d0ruk/graphql-demo.git"
  },
  "bugs": {
    "url": "https://github.com/d0ruk/graphql-demo/issues"
  },
  "homepage": "https://github.com/d0ruk/graphql-demo#readme"
}
